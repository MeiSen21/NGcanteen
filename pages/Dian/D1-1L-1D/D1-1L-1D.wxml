<!--pages/Dian/D1-1L-1D/D1-1L-1D.wxml-->
<view class="container">
  <block wx:for-items="{{list}}" wx:key="{{item.id}}">
    <navigator url="{{item.url}}">
    <view class="listitem">
   
        <view class="panel-image">
          <image src="{{item.src}}" class="imageitem"></image>
        </view>

        <view class="panel-right">
          <view class="panel-title">
            <view>{{item.title}}</view>
            <view>评分：{{item.score}} </view>        
          </view>
          <view class="panel-text">{{item.text}}</view>
        </view>        
      
    </view>
  </navigator>
  </block>

</view>