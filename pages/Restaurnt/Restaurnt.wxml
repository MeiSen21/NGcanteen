<!--pages/Restaurnt/Restaurnt.wxml-->
<view class="container">
  <block wx:for-items="{{Flist}}" wx:key="{{item.Rid}}">
    <navigator url="/pages/Dian/D1-1L-1D/D1-1L-1D?Rid={{item.Rid}}">
    <view class="listitem">
   
        <view class="panel-image">
          <image src="{{item.src}}" class="imageitem"></image>
        </view>

        <view class="panel-right">
          <view class="panel-title">
            <view>{{item.Rname}}</view>
            <view>{{item.Rcomment}} </view>        
          </view>
          <view class="panel-text">{{item.Rtext}}</view>
        </view>        
      
    </view>
  </navigator>
  </block>

</view>
